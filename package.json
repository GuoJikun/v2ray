{
  "name": "v2ray",
  "version": "1.0.0",
  "description": "使用v2ray-core开发的GUI界面",
  "main": "src/electron.js",
  "scripts": {
    "serve": "cross-env NODE_ENV=development electron .",
    "start": "electron-forge start",
    "release": "electron-forge import && electron-forge make",
    "dev": "vite",
    "build:web": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com.cnpmjs.org/GuoJikun/v2ray.git"
  },
  "keywords": [
    "v2ray",
    "v2ray-core",
    "gui"
  ],
  "author": "GuoJiKun",
  "license": "MIT",
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "element-plus": "^1.3.0-beta.5",
    "vue": "^3.2.25",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@types/node": "^17.0.10",
    "@vitejs/plugin-vue": "^2.0.0",
    "cross-env": "^7.0.3",
    "electron": "^16.0.7",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vue-tsc": "^0.29.8"
  }
}
